{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    // 包含的类型文件
    "include": [
        "components.d.ts",
        "auto-imports.d.ts",
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "compilerOptions": {
        "types": ["@types/wicg-file-system-access"],
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },

    "references": [
        {
            "path": "./tsconfig.config.json"
        }
    ]
}
